<?xml version="1.0" encoding="UTF-8"?>

<process-definition name="unusualOccurenceWorkflow">  
  <swimlane name="initiator"> 
    <assignment class="com.nas.recovery.web.action.workflowmgt.unusualOccurenceWorkflow.assign.InitiatorAssignment"/> 
  </swimlane>  
  <swimlane name="incidentReviewer"> 
    <assignment class="com.nas.recovery.web.action.workflowmgt.unusualOccurenceWorkflow.assign.IncidentReviewerAssignment"/> 
  </swimlane>  
  <start-state name=""> 
    <task name="" swimlane="initiator"></task>  
    <transition to="reviewOccurence" name="createIncident"></transition> 
  </start-state>  
  <task-node name="reviewOccurence"> 
    <task name="reviewOccurence" swimlane="incidentReviewer" notify="true"></task>  
    <transition to="provideInfo" name="requestMoreInfo"></transition>  
    <transition to="endState1" name="closeOccurence"></transition> 
  </task-node>  
  <task-node name="provideInfo"> 
    <task name="provideInfo" swimlane="initiator" notify="true"></task>  
    <transition to="reviewOccurence" name="done"></transition> 
  </task-node>  
  <end-state name="endState1"/> 
</process-definition>
